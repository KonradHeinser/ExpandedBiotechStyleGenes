<Patch>
    <Operation Class="PatchOperationConditional">
        <xpath>Defs/ThoughtDef[defName="AteWithoutTable"]/nullifyingGenes</xpath>
        <match Class="PatchOperationAdd">
            <xpath>Defs/ThoughtDef[defName="AteWithoutTable"]/nullifyingGenes</xpath>
            <value>
                <li>EBSG_MajorAsceticism</li>
                <li>EBSG_ExtremeAsceticism</li>
                <li>EBSG_SuperAsceticism</li>
                <li>EBSG_HyperAsceticism</li>
                <li MayRequire="EBSG.Archite">EBSG_MinorArchiteInducedAsceticism</li>
                <li MayRequire="EBSG.Archite">EBSG_ArchiteInducedAsceticism</li>
            </value>
        </match>
        <nomatch Class="PatchOperationAdd">
            <xpath>Defs/ThoughtDef[defName="AteWithoutTable"]</xpath>
            <value>
                <nullifyingGenes>
                    <li>EBSG_MajorAsceticism</li>

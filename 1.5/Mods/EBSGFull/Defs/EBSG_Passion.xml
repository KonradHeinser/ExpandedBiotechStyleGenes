<Defs>    
    <GeneDef Name="EBSG_Passion" Abstract="True">
        <geneClass>EBSGFramework.Gene_SkillChanging</geneClass>
        <displayCategory>EBSG_Passions</displayCategory>
        <description>Carriers of this gene have two levels of passion for this skill.</description>
        <biostatCpx>1</biostatCpx>
        <biostatMet>-2</biostatMet>
    </GeneDef>

    <GeneDef ParentName="EBSG_Passion">
        <defName>EBSG_Passion_Shooting</defName>
        <label>shooting passion</label>
        <iconPath>UI/Icons/Genes/Skills/Shooting/PassionAdd</iconPath>
        <displayOrderInCategory>1.6</displayOrderInCategory>
        <exclusionTags>
            <li>EBSG_Passion_Shooting</li>
        </exclusionTags>
        <modExtensions>
            <li Class="EBSGFramework.EBSGExtension">
                <skillChanges>
                    <li>
                        <skill>Shooting</skill>
                        <passionChange>2</passionChange>
                    </li>
                </skillChanges>
            </li>
	    </modExtensions>
    </GeneDef>


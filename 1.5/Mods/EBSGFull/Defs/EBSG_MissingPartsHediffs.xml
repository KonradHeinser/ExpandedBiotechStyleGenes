<?xml version="1.0" encoding="utf-8" ?>
<Defs>
    <HediffDef Name="EBSG_PartRemover" Abstract="True">
        <description>A body part will be damaged/removed once the game starts. To avoid weird errors, this needs to occur after the pawn is spawned.</description>
        <defaultLabelColor>(0.7, 0.7, 0.7)</defaultLabelColor>
		<hediffClass>HediffWithComps</hediffClass>
		<scenarioCanAdd>true</scenarioCanAdd>
		<maxSeverity>1.0</maxSeverity>
		<isBad>false</isBad>
    </HediffDef>

    <HediffDef ParentName="EBSG_PartRemover">
        <defName>EBSG_DamagedKidney</defName>
        <label>damaging kidney</label>
        <comps>
            <li Class="EBSGFramework.HediffCompProperties_DamageBodyParts">
                <bodyPartsToDamage>
                    <li>
                        <bodyPart>Kidney</bodyPart>
                        <damagePercentage>0.5</damagePercentage>
                    </li>
                </bodyPartsToDamage>
            </li>
        </comps>
    </HediffDef>

    <HediffDef ParentName="EBSG_PartRemover">
        <defName>EBSG_MissingKidney</defName>
        <label>removing kidney</label>
        <comps>
            <li Class="EBSGFramework.HediffCompProperties_DamageBodyParts">
                <bodyPartsToRemove>
                    <li>Kidney</li>
                </bodyPartsToRemove>
            </li>
        </comps>
    </HediffDef>


    <GeneDef Name="EBSG_Interest" Abstract="True">
        <geneClass>EBSGFramework.Gene_SkillChanging</geneClass>
        <displayCategory>EBSG_Passions</displayCategory>
        <description>Carriers of this gene have one more level of passion for this skill.</description>
        <biostatCpx>1</biostatCpx>
        <biostatMet>-1</biostatMet>
    </GeneDef>

    <GeneDef ParentName="EBSG_Interest">
        <defName>EBSG_Interest_Shooting</defName>
        <label>shooting interest</label>
        <iconPath>UI/Icons/Genes/Skills/Shooting/PassionAdd</iconPath>
        <displayOrderInCategory>1.3</displayOrderInCategory>
        <exclusionTags>
            <li>EBSG_Passion_Shooting</li>
        </exclusionTags>
        <modExtensions>
            <li Class="EBSGFramework.EBSGExtension">
                <skillChanges>
                    <li>
                        <skill>Shooting</skill>
                        <passionChange>1</passionChange>
                    </li>
                </skillChanges>
            </li>
	    </modExtensions>
    </GeneDef>


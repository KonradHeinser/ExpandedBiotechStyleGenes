<?xml version="1.0" encoding="utf-8" ?>
<Patch>
    <Operation Class="PatchOperationConditional">
        <xpath>Defs/GeneDef[defName="AG_PackMule"]</xpath>
        <match Class="PatchOperationAdd">
            <xpath>Defs/GeneDef[defName="AG_PackMule"]/modExtensions/li[@Class="VanillaGenesExpanded.GeneExtension"]</xpath>
            <value><hideGene>true</hideGene></value>
        </match>
    </Operation>

    <Operation Class="PatchOperationConditional">
        <xpath>Defs/GeneDef[defName="AG_FrailBack"]</xpath>
        <match Class="PatchOperationAdd">
            <xpath>Defs/GeneDef[defName="AG_FrailBack"]/modExtensions/li[@Class="VanillaGenesExpanded.GeneExtension"]</xpath>
            <value><hideGene>true</hideGene></value>
        </match>
    </Operation>
</Patch>
